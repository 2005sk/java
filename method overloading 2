// method overloading 
 Class parent () {
Void sum(int a) {
System.out.println(a) ;
}
void sum(int a, int b) {
System.out.println(a+b) ;
}
 void sum(int a, int b, intc) {
System.out.println(a+b+c) ;
}
void sum(double a,double b) {
System.out.println(a+b) 
}
public class method() {
public static void main (String[]args) {
parent obj=new parent() ;
obj.sum(34) ;
obj.sum(34,46) ;
obj.sum(34,46,78) ;
obj.sum(7.8,7.5) ;
}
}
}